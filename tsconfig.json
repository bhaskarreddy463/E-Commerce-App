{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015", "es2017"],
      "jsx": "react",
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "isolatedModules": true
    },
    "include": ["src/**/*.tsx", "src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  